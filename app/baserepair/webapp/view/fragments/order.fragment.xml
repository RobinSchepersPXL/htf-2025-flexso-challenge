<core:FragmentDefinition xmlns:smartfield="sap.ui.comp.smartfield"
    xmlns="sap.m"
    xmlns:f="sap.ui.layout.form"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core">
    <Dialog id="_IDGenDialog" title="Order" draggable="true">
        <content>
            <VBox class="sapUiSmallMargin" id="formContainer">
                <f:SimpleForm id="SimpleFormChangeColumn_twoGroups234" editable="true" layout="ColumnLayout" columnsM="2" columnsL="2" columnsXL="2">
                    <f:content>
                        <Label id="_IDGenLabel2" text="Amount" />
                        <Input id="_IDGenInput1" required="true" value="{order>/amount}" />
                    </f:content>
                </f:SimpleForm>
            </VBox>
        </content>
        <footer>
            <Toolbar id="_IDGenToolbar1">
                <content>
                    <ToolbarSpacer id="_IDGenToolbarSpacer4" />
                    <Button id="_IDGenButton3" type="Accept" text="Save" press="saveOrder" />
                    <Button id="_IDGenButton4" text="Reject" type="Reject" press="closeDialog" ariaHasPopup="Dialog" />
                </content>
            </Toolbar>
        </footer>
    </Dialog>
</core:FragmentDefinition>
